while True:
    for altcoin in altcoins:

        link = ("https://api.coinmarketcap.com/v1/ticker/%s" %altcoin)
        try:
            coinmarketcapdata = requests.get(link).json()
            Coinprice_usd = coinmarketcapdata[0]['price_usd']
            Coinprice_btc = coinmarketcapdata[0]['price_btc']
            Coinrank = coinmarketcapdata[0]['rank']
            Coin24h_volume_usd = coinmarketcapdata[0]['24h_volume_usd']
            Coinmarket_cap_usd = coinmarketcapdata[0]['market_cap_usd']
            Cointotal_supply = coinmarketcapdata[0]['total_supply']
            Coinpercent_change_1h = coinmarketcapdata[0]['percent_change_1h']
            Coinpercent_change_24h = coinmarketcapdata[0]['percent_change_24h']
            Coinpercent_change_7d = coinmarketcapdata[0]['percent_change_7d']

            volume_compared_to_Mcap = (float(Coin24h_volume_usd)/float(Coinmarket_cap_usd))*100
            change_in_1h_vs_24h = (float(Coinpercent_change_1h)/float(Coinpercent_change_24h))*100
            change_in_1h_vs_7d = (float(Coinpercent_change_1h)/float(Coinpercent_change_7d))*100
            change_in_24h_vs_7d = (float(Coinpercent_change_24h)/float(Coinpercent_change_7d))*100

            PDscore = volume_compared_to_Mcap * Volvsmc + change_in_1h_vs_24h * onehvsoneday + change_in_1h_vs_7d * onehvsoneweek + change_in_24h_vs_7d * onedayvsoneweek
            
            if PDscore > 1:
                print('Coin: ' + altcoin)
                print('price_usd: ' + Coinprice_usd)
                print('price_btc: ' + Coinprice_btc)
                print('rank: ' + Coinrank)
                print('24h_volume_usd: ' + Coin24h_volume_usd)
                print('market_cap_usd: ' + Coinmarket_cap_usd)
                print('total_supply: ' + Cointotal_supply)
                print('percent_change_1h: ' + Coinpercent_change_1h)
                print('percent_change_24h: ' + Coinpercent_change_24h)
                print('percent_change_7d: ' + Coinpercent_change_7d)
                print('\n')
                print('volume compared to Mcap: ' + str(volume_compared_to_Mcap))
                print('change in 1h vs 24h: ' + str(change_in_1h_vs_24h))
                print('change in 1h vs 7 days: ' + str(change_in_1h_vs_7d))
                print('change in 24h vs 7 days: ' + str(change_in_24h_vs_7d))
                print('pump and Dump score: ')
                print(PDscore)
                print('\n')
            else:
                continue
        except:
            continue
        time.sleep(0.1)
        

